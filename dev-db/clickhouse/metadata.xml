<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE pkgmetadata SYSTEM "http://www.gentoo.org/dtd/metadata.dtd">
<pkgmetadata>
	<upstream>
		<remote-id type="github">yandex/Clickhouse</remote-id>
	</upstream>
	<use>
		<flag name="client">Build CLI client.</flag>
		<flag name="jemalloc">Use jemalloc library for memory allocation.</flag>
		<flag name="kafka">Enable support for kafka consumer.</flag>
		<flag name="lld">Use llvm lld linker for build.</flag>
		<flag name="lto">Enable lto optimizations.</flag>
		<flag name="mongodb">Enable support for MongoDB-based dictionaries.</flag>
		<flag name="parquet">Enable parquet.</flag>
		<flag name="redis">Enable support for Redis-based dictionaries.</flag>
		<flag name="s3">Enable S3.</flag>
		<flag name="server">Build server.</flag>
		<flag name="system-capnproto">Use system-wide capnproto library.</flag>
		<flag name="system-double-conversion">Use system-wide double-conversion library.</flag>
